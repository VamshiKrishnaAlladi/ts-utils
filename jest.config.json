{
    "collectCoverage": true,
    "coverageThreshold": {
        "global": {
            "branches": 100,
            "functions": 100,
            "lines": 100,
            "statements": 100
        }
    },
    "globals": {
        "ts-jest": {
            "diagnostics": false,
            "tsConfig": "./ts-jest.tsconfig.json"
        }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "preset": "ts-jest",
    "testMatch": null
}
